{
  "name": "erp",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode dev",
    "build": "vue-cli-service build --mode prod",
    "electron:build": "vue-cli-service electron:build -w --mode dev",
    "electron:serve": "vue-cli-service electron:serve -w --mode dev",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "prisma:chain": "npm run prisma:db:push:force && npm run prisma:introspect && npm run prisma:generate && npm run serve",
    "prisma:db:push": "npx prisma db push",
    "prisma:db:pull": "npx prisma db pull",
    "prisma:db:push:force": "npx prisma db push --accept-data-loss",
    "prisma:db:seed": "npx prisma db seed --preview-feature",
    "prisma:generate": "npx prisma generate",
    "prisma:init": "npx prisma migrate dev && PRISMA_CLI_BINARY_TARGETS=darwin,darwin-arm64 npm install @prisma/engines",
    "prisma:studio": "npx prisma studio"
  },
  "main": "background.js",
  "dependencies": {
    "@electron/remote": "^2.0.4",
    "@element-plus/icons": "0.0.11",
    "@element-plus/icons-vue": "^0.2.4",
    "@prisma/client": "^3.8.1",
    "@vueuse/core": "^7.5.5",
    "axios": "^0.21.4",
    "core-js": "^3.6.5",
    "decimal.js": "^10.3.1",
    "echarts": "^5.0.2",
    "element-plus": "^1.2.0-beta.6",
    "lodash": "^4.17.21",
    "mathjs": "^10.0.1",
    "moment": "^2.29.1",
    "register-service-worker": "^1.7.1",
    "v-contextmenu": "^3.0.0",
    "vue": "^3.0.0",
    "vue-request": "^1.2.4",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "electron": "^13.0.0",
    "electron-devtools-installer": "^3.1.0",
    "node-sass": "^4.12.0",
    "prisma": "^3.9.1",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^5.3.1",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
